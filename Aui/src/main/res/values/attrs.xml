<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="SwipeAdapterView">
    <attr name="ViewMode" format="enum">
      <enum name="List" value="0"/>
      <enum name="Grid" value="1"/>
    </attr>
    <attr name="PromptMode" format="enum">
      <enum name="TextOnly" value="0"/>
      <enum name="ImageOnly" value="1"/>
      <enum name="ImageTop_TextBottom" value="2"/>
      <enum name="ImageBottom_TextTop" value="3"/>
      <enum name="ImageLeft_TextRight" value="4"/>
      <enum name="ImageRight_TextLeft" value="5"/>
    </attr>
    <attr name="FooterMode" format="enum">
      <enum name="Disabled" value="0"/>
      <enum name="Docked" value="1"/>
      <enum name="Scrolling" value="2"/>
    </attr>
    <attr name="TranscriptMode" format="enum">
      <enum name="Disabled" value="0"/>
      <enum name="Normal" value="1"/>
      <enum name="AlwaysScroll" value="2"/>
    </attr>
    <attr name="FooterHeight" format="dimension|reference"/>
    <attr name="FooterBackground" format="reference"/>
    <attr name="FloatingButtonBackground" format="reference"/>
    <attr name="FloatingButtonEnabled" format="boolean"/>
  </declare-styleable>
</resources>